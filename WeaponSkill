CMD:uparskill(playerid, params[])
{
SetPlayerSkillLevelEx(playerid, 0, SkillLevel[playerid]++);
format(string2, sizeof (string2), "colt45 skill %d", SkillLevel[playerid]);
SendClientMessage(playerid, -1, string2);
return 1;
}

// vai mostrar o progresso do jogador com cada arma em sua mão

CMD:skill(playerid, params[])
{
if(GetPlayerWeapon(playerid) == 22)
{
format(string2, sizeof (string2), "colt45 skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 23)
{
format(string2, sizeof (string2), "silenced skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 24)
{
format(string2, sizeof (string2), "desert skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 25)
{
format(string2, sizeof (string2), "shotgun skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 26)
{
format(string2, sizeof (string2), "swanoff skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 27)
{
format(string2, sizeof (string2), "shotspa skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 28)
{
format(string2, sizeof (string2), "uzi skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 29)
{
format(string2, sizeof (string2), "mp5 skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 30)
{
format(string2, sizeof (string2), "ak47 skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 31)
{
format(string2, sizeof (string2), "m4 skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
if(GetPlayerWeapon(playerid) == 34)
{
format(string2, sizeof (string2), "sniper skill %d", WeaponSkill[playerid]);
SendClientMessage(playerid, -1, string2);
}
return 1;
}

CMD:setskill(playerid, params[])
{
new pid, wid, level;
if(sscanf(params, "udd", pid, wid, level))
{
SendClientMessage(playerid,0xFFFF00FF,"/setskill [playerid] [weaponid] [level]");
return 1;
}
SetPlayerSkillLevelEx(pid, wid, level);
SkillLevel[pid] = level;
return 1;
}

// mostra o estado geral das habilidades dele com a arma

CMD:myskill(playerid, params[])
{
format(string2, sizeof (string2), "Seu nível de habilidade atual %d", SkillLevel[playerid]);
SendClientMessage(playerid, -1, string2);
return 1;
}

// reporta o estado atual das habilidades com as armas de cada jogador

CMD:playerskill(playerid, params[])
{
new pid;
if(sscanf(params, "u", pid))
{
SendClientMessage(playerid,0xFFFF00FF,"/playerskill [jogador id]");
return 1;
}
GetPlayerName(pid, playername, 24);
format(string2, sizeof (string2), "%s Pistol: %d Silenced: %d Desert: %d Shotgun: %d Sawnoff: %d Spas12: %d Uzi - Tec9: %d", playername, pistol_skill[pid], silenced_skill[pid], desert_skill[pid], shotgun_skill[pid], sawnoff_skill[pid], spas12_skill[pid], uzi_skill[pid]);
SendClientMessage(playerid, -1, string2);
format(string2, sizeof (string2), "mp5: %d ak47: %d m4: %d Sniper: %d", mp5_skill[pid], ak47_skill[pid], m4_skill[pid], sniper_skill[pid]);
SendClientMessage(playerid, -1, string2);
return 1;
}

forward reportarid(playerid);

/* nunca use isso pra kickar / banir o jogador porque mesmo que ele não se mova ele ainda vai ser reportado apertando W A S D enquanto atira, e essa é a ideia por trás desse sistema
os jogadores vão ser avisados e alguém tem que ficar de /tv monitorando a animação do ped armado pra ver se ta xitando ou não */

public OnPlayerWeaponShot(playerid, weaponid, hittype, hitid, Float:fX, Float:fY, Float:fZ)
{
// o resto das armas será ignorado estas são as que importam pro sistema funcionar
if(GetPlayerWeapon(playerid) == 22 || GetPlayerWeapon(playerid) == 23 || GetPlayerWeapon(playerid) == 24 || GetPlayerWeapon(playerid) == 25 || GetPlayerWeapon(playerid) == 27 || 
GetPlayerWeapon(playerid) == 28 || GetPlayerWeapon(playerid) == 29 ||  GetPlayerWeapon(playerid) == 30 || GetPlayerWeapon(playerid) == 31 || GetPlayerWeapon(playerid) == 32 || 
GetPlayerWeapon(playerid) == 34)
{
if(SkillLevel[playerid] < 998)
{
new Keys, ud, lr;
GetPlayerKeys(playerid, Keys, ud, lr);
// essa função será executada uma unica vez toda vez que o jogador entrar no servidor
if(reported[playerid] == 0)
{
SetTimer("reportarid", 1000, false);
if(ud == KEY_UP)
{
SendClientMessage(playerid, -1, "Reportado por sispeita de Weapon S Hack!");
SendClientMessage(playerid, -1, "Você não pode atirar e se mover ao mesmo tempo!");
reported[playerid] = 1;
}
if(ud == KEY_DOWN)
{
SendClientMessage(playerid, -1, "Reportado por sispeita de Weapon S Hack!");
SendClientMessage(playerid, -1, "Você não pode atirar e se mover ao mesmo tempo!");
reported[playerid] = 1;
}
if(lr == KEY_LEFT)
{
SendClientMessage(playerid, -1, "Reportado por sispeita de Weapon S Hack!");
SendClientMessage(playerid, -1, "Você não pode atirar e se mover ao mesmo tempo!");
reported[playerid] = 1;
}
if(lr == KEY_RIGHT)
{
SendClientMessage(playerid, -1, "Reportado por sispeita de Weapon S Hack!");
SendClientMessage(playerid, -1, "Você não pode atirar e se mover ao mesmo tempo!");
reported[playerid] = 1;
}
}
}
}
return 1;
}

public reportarid(playerid)
{
format(string2, sizeof (string2), "%s foi reportado por suspeita Weapon S Hack!", playername);
SendClientMessageToAll( -1, string2);
SendClientMessageToAll(-1, "Para comprovar recordem com /tv");
return 1;
}

// Créditos: Nibblet
// eu tava usando isso porque tinha esquecido como fazer o operador de soma funcionar remova se quiser quando for adaptar isso pro seu gm

stock SetPlayerSkillLevelEx(playerid, skill, level)
{
if(IsPlayerConnected(playerid))
{
SetPlayerSkillLevel(playerid, skill, level);
WeaponSkill[playerid][skill] = level;    
}
return 1;
}

stock GetPlayerSkillLevelEx(playerid, skill)
{
if(IsPlayerConnected(playerid))
{
return WeaponSkill[playerid][skill];
}
return 1;
}

// aqui você vai fazer a progressão do jogador do zero até 999

new pdeaths[24]; 

public OnPlayerDeath(playerid, killerid, reason)
{
if(killerid != INVALID_PLAYER_ID)
{
pdeaths[killerid]++;
SkillLevel[killerid]++;

// se você quiser que vá do zero até 999 deixe isso funcionando e bote no seu sistema de salvamento

if(GetPlayerWeapon(killerid) == 22) SetPlayerSkillLevelEx(killerid, 0, SkillLevel[killerid]++); // pistol
if(GetPlayerWeapon(killerid) == 23) SetPlayerSkillLevelEx(killerid, 1, SkillLevel[killerid]++); // silenced
if(GetPlayerWeapon(killerid) == 24) SetPlayerSkillLevelEx(killerid, 2, SkillLevel[killerid]++); // desert
if(GetPlayerWeapon(killerid) == 25) SetPlayerSkillLevelEx(killerid, 3, SkillLevel[killerid]++); // shotgun
if(GetPlayerWeapon(killerid) == 26) SetPlayerSkillLevelEx(killerid, 4, SkillLevel[killerid]++); // sawnoff
if(GetPlayerWeapon(killerid) == 27) SetPlayerSkillLevelEx(killerid, 5, SkillLevel[killerid]++); // spas12 - combat shotgun
if(GetPlayerWeapon(killerid) == 28 || GetPlayerWeapon(killerid) == 32) SetPlayerSkillLevelEx(killerid, 6, SkillLevel[killerid]++); // uzi - tec9
if(GetPlayerWeapon(killerid) == 29) SetPlayerSkillLevelEx(killerid, 7, SkillLevel[killerid]++); // mp5
if(GetPlayerWeapon(killerid) == 30) SetPlayerSkillLevelEx(killerid, 8, SkillLevel[killerid]++); // ak47
if(GetPlayerWeapon(killerid) == 31) SetPlayerSkillLevelEx(killerid, 9, SkillLevel[killerid]++); // m4
if(GetPlayerWeapon(killerid) == 34) SetPlayerSkillLevelEx(killerid, 10, SkillLevel[killerid]++); // sniper

// aqui em cada arma ele tem que matar 36 jogadores pra chegar no nível hitman

if(pdeaths[killerid] > 1)
{
SkillLevel[killerid] = 10;
if(GetPlayerWeapon(killerid) == 22)
{
SetPlayerSkillLevelEx(killerid, 0, 10); // 22
pistol_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 23)
{
SetPlayerSkillLevelEx(killerid, 1, 10); // 23
silenced_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 24)
{
SetPlayerSkillLevelEx(killerid, 2, 10); // 24
desert_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 25)
{
SetPlayerSkillLevelEx(killerid, 3, 10); // 25
shotgun_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 26)
{
SetPlayerSkillLevelEx(killerid, 4, 10); // 26
sawnoff_skill[killerid] = 10; 
}
if(GetPlayerWeapon(killerid) == 27)
{
SetPlayerSkillLevelEx(killerid, 5, 10); // 27
spas12_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 28 || GetPlayerWeapon(killerid) == 32)
{
SetPlayerSkillLevelEx(killerid, 6, 10); // 28 - 32
uzi_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 29)
{
SetPlayerSkillLevelEx(killerid, 7, 10); // 29
mp5_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 30)
{
SetPlayerSkillLevelEx(killerid, 8, 10); // 30
ak47_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 31)
{
SetPlayerSkillLevelEx(killerid, 9, 10); // 31
m4_skill[killerid] = 10;
}
if(GetPlayerWeapon(killerid) == 34)
{
SetPlayerSkillLevelEx(killerid, 10, 10); // 34
sniper_skill[killerid] = 10;
}
}
//
if(pdeaths[killerid] > 2)
{
SkillLevel[killerid] = 20;
if(GetPlayerWeapon(killerid) == 22)
{
SetPlayerSkillLevelEx(killerid, 0, 20); // 22 Pistol
pistol_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 23)
{
SetPlayerSkillLevelEx(killerid, 1, 20); // 23 Silenced
silenced_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 24)
{
SetPlayerSkillLevelEx(killerid, 2, 20); // 24 Desert
desert_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 25)
{
SetPlayerSkillLevelEx(killerid, 3, 20); // 25 Shotgun
shotgun_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 26)
{
SetPlayerSkillLevelEx(killerid, 4, 20); // 26 Sawnoff
sawnoff_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 27)
{
SetPlayerSkillLevelEx(killerid, 5, 20); // 27 Spas12
spas12_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 28 || GetPlayerWeapon(killerid) == 32)
{
SetPlayerSkillLevelEx(killerid, 6, 20); // 28 UZI / 32 Tec9
uzi_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 29)
{
SetPlayerSkillLevelEx(killerid, 7, 20); // 29 MP5
mp5_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 30)
{
SetPlayerSkillLevelEx(killerid, 8, 20); // 30 Ak47
ak47_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 31)
{
SetPlayerSkillLevelEx(killerid, 9, 20); // 31 M4
m4_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 34)
{
SetPlayerSkillLevelEx(killerid, 10, 20); // 34 Sniper
sniper_skill[killerid] = 20;
}
}
//
if(pdeaths[killerid] > 3)
{
SkillLevel[killerid] = 30;
if(GetPlayerWeapon(killerid) == 22)
{
SetPlayerSkillLevelEx(killerid, 0, 30); // 22
pistol_skill[killerid] = 30;
}
if(GetPlayerWeapon(killerid) == 23)
{
SetPlayerSkillLevelEx(killerid, 1, 30); // 23
silenced_skill[killerid] = 20;
}
if(GetPlayerWeapon(killerid) == 24)
{
SetPlayerSkillLevelEx(killerid, 2, 30); // 24
}
if(GetPlayerWeapon(killerid) == 25)
{
SetPlayerSkillLevelEx(killerid, 3, 30); // 25
}
if(GetPlayerWeapon(killerid) == 26)
{
SetPlayerSkillLevelEx(killerid, 4, 30); // 26
}
if(GetPlayerWeapon(killerid) == 27)
{
SetPlayerSkillLevelEx(killerid, 5, 30); // 27
}
if(GetPlayerWeapon(killerid) == 28 || GetPlayerWeapon(killerid) == 32)
{
SetPlayerSkillLevelEx(killerid, 6, 30); // 28 - 32
}
if(GetPlayerWeapon(killerid) == 29)
{
SetPlayerSkillLevelEx(killerid, 7, 10); // 29
}
if(GetPlayerWeapon(killerid) == 30)
{
SetPlayerSkillLevelEx(killerid, 8, 30); // 30
}
if(GetPlayerWeapon(killerid) == 31)
{
SetPlayerSkillLevelEx(killerid, 9, 30); // 31
}
if(GetPlayerWeapon(killerid) == 34)
{
SetPlayerSkillLevelEx(killerid, 10, 30); // 34
}
}
//
if(pdeaths[killerid] > 4)
{
SkillLevel[killerid] = 40;
if(GetPlayerWeapon(killerid) == 22)
{
SetPlayerSkillLevelEx(killerid, 0, 40); // 22
pistol_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 23)
{
SetPlayerSkillLevelEx(killerid, 1, 40); // 23
silenced_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 24)
{
SetPlayerSkillLevelEx(killerid, 2, 40); // 24
desert_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 25)
{
SetPlayerSkillLevelEx(killerid, 3, 40); // 25
shotgun_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 26)
{
SetPlayerSkillLevelEx(killerid, 4, 40); // 26
sawnoff_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 27)
{
SetPlayerSkillLevelEx(killerid, 5, 40); // 27
spas12_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 28 || GetPlayerWeapon(killerid) == 32)
{
SetPlayerSkillLevelEx(killerid, 6, 40); // 28 - 32
uzi_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 29)
{
SetPlayerSkillLevelEx(killerid, 7, 40); // 29
mp5_skill[killerid] = 40;
}
if(GetPlayerWeapon(killerid) == 30)
